{
  "name": "Audio extension",
  "version": "0.1.0",
  "description": "An extension that recognizes audio from a tab",
  "permissions": [
    "activeTab",
    "tabCapture",
    "tabs"
  ],
 
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
  }],
  
  "icons": {
      "16": "test_images/icon-16.png",
      "48": "test_images/icon-48.png",
      "32": "test_images/icon-32.png",
      "128": "test_images/icon-128.png"
  },
  "action": {
      "default_icon": {
          "16": "icon-16.png",
          "48": "icon-48.png",
          "32": "icon-32.png",
          "128": "icon-128.png"
      },
      "default_title": "Audio title",
      "default_popup": "index.html"
  },
  "manifest_version": 3
}